    MainContainer.Position = UDim2.new(0.340923071, 0, 0.274853796, 0)
    MainContainer.Size = UDim2.new(0, 517, 0, 385)
    
    PlayerIcon.Name = "PlayerIcon"
    PlayerIcon.Parent = MainContainer
    PlayerIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    PlayerIcon.BackgroundTransparency = 1
    PlayerIcon.BorderSizePixel = 0
    PlayerIcon.Position = UDim2.new(0.0406189561, 0, 0.0545454547, 0)
    PlayerIcon.Size = UDim2.new(0, 78, 0, 78)
    PlayerIcon.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
    
    UICorner.CornerRadius = UDim.new(1, 0)
    UICorner.Parent = PlayerIcon
    
    PlayerName.Name = "PlayerName"
    PlayerName.Parent = MainContainer
