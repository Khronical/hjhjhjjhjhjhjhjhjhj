    OSbutton.Name = "OSbutton"
    OSbutton.Parent = MainContainer
    OSbutton.BackgroundColor3 = Color3.fromRGB(44, 164, 255)
    OSbutton.BorderSizePixel = 0
    OSbutton.Position = UDim2.new(0.0986460373, 0, 0.555844188, 0)
    OSbutton.Size = UDim2.new(0, 200, 0, 41)
    OSbutton.Font = Enum.Font.GothamBold
    OSbutton.Text = "Orbital Strike"
    OSbutton.TextColor3 = Color3.fromRGB(255, 255, 255)
    OSbutton.TextSize = 25.000
    OSbutton.MouseButton1Down:Connect(function()
        IFL:Orbital(PlayerName.Text)
    end)
    
    UICorner_2.CornerRadius = UDim.new(0, 3)
    UICorner_2.Parent = OSbutton
    
    CloseButton.Name = "CloseButton"
    CloseButton.Parent = MainContainer
    CloseButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.BackgroundTransparency = 1.000
    CloseButton.BorderSizePixel = 0
    CloseButton.Position = UDim2.new(0.922630548, 0, 0, 0)
    CloseButton.Size = UDim2.new(0, 40, 0, 35)
    CloseButton.Font = Enum.Font.SourceSans
    CloseButton.Text = "X"
    CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.TextSize = 54.000
    CloseButton.TextWrapped = true
    CloseButton.MouseButton1Down:Connect(function()
        for i, v in pairs(game:GetService("CoreGui"):GetDescendants()) do
            if v:IsA("TextButton") and v.Text == "Orbital Strike" then
                v.Parent.Parent:Destroy()
            end
        end
    end)
    
    UIAspectRatioConstraint.Parent = MainContainer
    UIAspectRatioConstraint.AspectRatio = 1.343
    
    for _, v in pairs(game:GetService("Players"):GetChildren()) do
        local TextButton = Instance.new("TextButton")
    
        TextButton.Name = "PlayerButton"
        TextButton.Parent = PlayerList
        TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.BackgroundTransparency = 0.300
        TextButton.BorderSizePixel = 0
        TextButton.Size = UDim2.new(0, 200, 0, 50)
        TextButton.Font = Enum.Font.GothamBold
        TextButton.Text = v.Name
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextScaled = true
        TextButton.TextSize = 14.000
        TextButton.TextStrokeTransparency = 0.000
        TextButton.TextWrapped = true
        TextButton.MouseButton1Down:Connect(function()
            PlayerName.Text = v.Name
            PlayerIcon.Image = "https://www.roblox.com/bust-thumbnail/image?userId=".. game.Players[PlayerName.Text].UserId .."&width=420&height=420&format=png"
        end)
    end
    
    local UIS = game:GetService('UserInputService')
    local frame = MainContainer
    local dragToggle = nil
    local dragSpeed = 0.25
    local dragStart = nil
    local startPos = nil
    
    local function updateInput(input)
     local delta = input.Position - dragStart
     local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
      startPos.Y.Scale, startPos.Y.Offset + delta.Y)
     game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
    end
    
    frame.InputBegan:Connect(function(input)
     if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
      dragToggle = true
      dragStart = input.Position
      startPos = frame.Position
      input.Changed:Connect(function()
       if input.UserInputState == Enum.UserInputState.End then
        dragToggle = false
       end
      end)
     end
    end)
    
    UIS.InputChanged:Connect(function(input)
     if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
      if dragToggle then
       updateInput(input)
      end
     end
    end)
    
    local UIGridLayout = Instance.new("UIGridLayout")
    UIGridLayout.Parent = PlayerList
    UIGridLayout.CellSize = UDim2.new(0, 190, 0, 40)
    for i, v in pairs(RandomString:GetDescendants()) do
        v.Name = IFL:randomstring()
    end
    
    while wait() do
        for i, v in pairs(PlayerList:GetChildren()) do
            if v:IsA("TextButton") and game:GetService("Players"):FindFirstChild(v.Text) == nil then
                v:Destroy()
            end
        end
        if game:GetService("Players"):FindFirstChild(PlayerName.Text) then
            OC.Text = "Tank Oxygen Consumed : ".. IFL:getUsedOxygen(PlayerName.Text)
            PK.Text = "Distance Traveled : ".. IFL:getTravelDistance(PlayerName.Text)
            DT.Text = "Player Kills : ".. IFL:getKills(PlayerName.Text)
            HEALTH.Text = "Health : " .. game:GetService("Players")[PlayerName.Text].Character.Humanoid.Health
            DA.Text = "Distance Away : " .. math.ceil(game.Players[PlayerName.Text]:DistanceFromCharacter(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position))
        end
    end
    end
    
