section1:addToggle("Toggle Sleep", nil, function(value)
    if(value == true)
    then
        game.Players.LocalPlayer.Character.SleepMode.Value = true
        game.Players.LocalPlayer.PlayerGui.Game.Sleep.Visible = false
    end

    if(value == false)
    then
        game.Players.LocalPlayer.Character.SleepMode.Value = false
        game.Players.LocalPlayer.PlayerGui.Game.Sleep.Visible = false
    end
print("Toggled", value)
end)

section1:addToggle("Scramble Locked Door", nil, function(value)
    while true do
        if value == true then
            wait(.1)
            local f = game.Workspace.Gate.Buttons["5"].ClickDetector
            local m = game.Workspace.Gate.Buttons["7"].ClickDetector
            fireclickdetector(f)
            fireclickdetector(m)
        end
    end
end)

