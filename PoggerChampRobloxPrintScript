    sectiones9:addButton(v.Name, function()
	    if v:IsA("Part") or v:IsA("MeshPart") or v:IsA("UnionOperation") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
        else
            if v:IsA("Model") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild().CFrame
            end
        end
    end)
end

