if game.CoreGui:FindFirstChild('ScreenGui') then
    game.CoreGui.ScreenGui:Destroy()
end

local info = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId, Enum.InfoType.Asset)

local ScreenGui = Instance.new("ScreenGui")
local ImageLabel = Instance.new("ImageLabel")
local UICorner = Instance.new("UICorner")
local TextLabel = Instance.new("TextLabel")

local StartPos = UDim2.new(0.43, 0,-0.404, 0)
local EndPos = UDim2.new(0.43, 0,0.348, 0)

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ImageLabel.Parent = ScreenGui
ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ImageLabel.Position = StartPos
ImageLabel.Size = UDim2.new(0, 232, 0, 221)
ImageLabel.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
UICorner.Parent = ImageLabel
TextLabel.Parent = ImageLabel
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(27, 42, 53)
TextLabel.BorderSizePixel = 0
TextLabel.Position = UDim2.new(0, 0, 1.00549817, 0)
TextLabel.Size = UDim2.new(0, 232, 0, 61)
TextLabel.Font = Enum.Font.SourceSansBold
TextLabel.Text = info.Name.." Module"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextScaled = true
TextLabel.TextSize = 14.000
TextLabel.TextWrapped = true

local Product = game:GetService("MarketplaceService")

local Details = Product:GetProductInfo(game.PlaceId)
local IconID = Details.IconImageAssetId

if games[game.PlaceId] then
    if info.Name == 'Game' then
        ImageLabel.Image = "rbxassetid://"..game:GetService("MarketplaceService"):GetProductInfo(3095204897).IconImageAssetId
    end
    ImageLabel:TweenPosition(EndPos, "InOut", "Bounce", 0.5)
    wait(3)
    ImageLabel:TweenPosition(StartPos, "InOut", "Elastic", 0.5)
    pcall(games[game.PlaceId]["function"])
end
